package chap8;

public class Person {
	Person(){}
	Person(int age){
		this.age = age;
	}
	
	Person(int age, String name){
		this(age);
		this.name = name;
	}
	
	Person(int age, String name, float weight){
		this(age, name);
		this.name = name;
	}
	
	int age;
	String name;
	float weight;
	
	public int getAge() {
		return age;
	}
	
	public void setAge(int age) {
		this.age = age;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public float getWeight() {
		return weight;
	}
	
	public void setWeight(float weight) { 
		this.weight = weight;
	}
	
	public void print() {
		System.out.println("age : " + age + "name : " + name);
	}
	
	
	
}
