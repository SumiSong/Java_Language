package chap8;

public class Person {
	Person(){}
	Person(int age, String name){
		this.age = age;
		this.name = name;
	}
	
	int age;
	String name;
	String 
	
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public void print() {
		System.out.println("age : " + age + "name : " + name);
	}
	
	
}
