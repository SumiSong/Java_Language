package chap8;

public class Animal {
	public String text;

	public void cry(String text) {
		this.text = text;
		System.out.println(text);
	}
	
}



class Bird extends Animal {
	
	public void cry() {
		super.cry(text);
		
	}
}

 class Cat extends Animal {
	public void cry() {
		super.cry(text);
	}
}